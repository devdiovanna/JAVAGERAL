package TelaValores2;

public class TelaValores2 {
    public static void main(String[] args) {



        float v  = 8.6f;
        int ar = (int) Math.round (v);
        System.out.println(ar);
        double ale = Math.random();
        int n = (int) (15 + ale * (50-15));
        System.out.println(n);
    }
}
