    @Override
    public void desenhar() {
        // Desenha o retângulo
    }

    @Override
    public void mover(int x, int y) {
        // Move o retângulo
    }

    @Override
    public void redimensionar(int largura, int altura) {
        // Redimensiona o retângulo
    }
}
