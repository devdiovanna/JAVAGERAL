public class Maiin {
    public static void main(String[] args) {
        int[] meuVetor = {10, 20, 30, 40, 50}; // Declare and initialize the array

        for (int i = 0; i < meuVetor.length; i++) { // Corrected loop condition
            System.out.println("Elemento" + i + ": " + meuVetor[i]); // Corrected output format
        }
    }
}
