import org.junit.Test;

import static org.junit.Assert.assertEquals;

public class PessoaTest {

    @Test
    public void deveRetornarONomeDoObjeto() {
        // Cria um objeto da classe Pessoa
        Pessoa p1 = new Pessoa("Miguel", 42);

        // Espera que o m√©todo getNome() retorne o nome do objeto
        assertEquals("Miguel", p1.getNome());
    }
}
