public class SomaSimples {

    public static int soma(int n1, int n2) {
        return n1 + n2;
    }

    public static void main(String[] args) {
        int n1 = 10;
        int n2 = 20;

        int soma = soma(n1, n2);

        System.out.println("A soma de " + n1 + " e " + n2 + " Ã© " + soma);
    }
}
