package Double;

import java.util.Arrays;

public class Double {
    public static void main(String[] args) {
        int[] vet = {3, 7, 8, 5, 4};

        for (int v : vet) {
            System.out.print(v + " ");
        }

        System.out.println();

        int p = Arrays.binarySearch(vet, 8);

        if (p >= 0) {
            System.out.println("Encontrei o valor na posição " + p);
        } else {
            System.out.println("Valor não encontrado");
        }
    }
}
